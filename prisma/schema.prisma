datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id Int @default(autoincrement()) @id
  title String
  description String?
  price Decimal @db.Money
  qtt Int
}